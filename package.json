{
  "name": "vue-skeleton-mvp",
  "version": "5.0.10",
  "description": "VueJS Vuetify Vue Router Vuex - Basic Project Skeleton - Starter Web App. Written on JavaScript using async/await.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davellanedam/vue-skeleton-mvp.git"
  },
  "scripts": {
    "generate-build-version": "node generate-build-version",
    "serve": "vue-cli-service serve",
    "build": "npm run generate-build-version && vue-cli-service build",
    "format": "prettier --write --config .prettierrc \"**/*.js\"",
    "lint": "vue-cli-service lint --fix",
    "test:e2e": "vue-cli-service test:e2e --mode e2e",
    "test:unit": "vue-cli-service test:unit",
    "remark": "remark . -o",
    "ci": "vue-cli-service test:e2e --env API_URL=https://api-demo.daniel-avellaneda.com,ENV=ci --headless --config video=false",
    "coverage": "nyc report --reporter=text-summary"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run remark"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@mdi/font": "^5.6.55",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "remark-cli": "^8.0.1",
    "vee-validate": "^3.4.0",
    "vue": "^2.6.12",
    "vue-analytics": "^5.22.1",
    "vue-country-flag": "^2.0.1",
    "vue-i18n": "^8.21.1",
    "vue-meta": "^2.4.0",
    "vue-resize-text": "^0.1.1",
    "vue-router": "^3.4.5",
    "vuetify": "^2.3.12",
    "vuetify-confirm": "^2.0.6",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-e2e-cypress": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-unit-mocha": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0",
    "faker": "^5.1.0",
    "fibers": "^5.0.0",
    "husky": "^4.3.0",
    "istanbul-lib-coverage": "^3.0.0",
    "lint-staged": "^10.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.1.0",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0"
  },
  "keywords": [
    "vuejs",
    "vue-router",
    "vuex",
    "vuetify",
    "javascript",
    "i18n",
    "eslint",
    "skeleton",
    "async",
    "starter",
    "webapp",
    "web",
    "app",
    "await",
    "mvp",
    "frontend",
    "testing",
    "prettier",
    "cypress",
    "mocha",
    "chai"
  ]
}
